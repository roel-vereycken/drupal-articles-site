<<header  class="main-header" role="banner">
  <div class="container">
    {{ page.header }}
  </div>
</header>

<div class="main-navigation">
  <div class="navLeft">
    <img class="navLogo" src="http://via.placeholder.com/30x30" alt="">
  </div>

  <div class="navRight">
    {{ page.main_navigation }}
  </div>
</div>

<div class="container">
  <main role="main">
    <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

    <div class="content">
      {{ page.content }}
    </div>{# /.layout-content #}

    {% if page.sidebar %}
      <aside class="layout-sidebar-first" role="complementary">
        {{ page.sidebar }}
      </aside>
    {% endif %}


  </main>

  {% if page.footer %}
    <footer role="contentinfo">
      {{ page.footer }}
    </footer>
  {% endif %}

</div>{# /.layout-container #}

